.hud-container {
    width: 100%;
    height: 100%;
    align-items: center;
    justify-content: center;
}

.hud-inventory {
    position: absolute;
    bottom: 40px;
    flex-direction: row;
    align-items: flex-end;
    left: 670px;
}

.hud-weapon-slot {
    width: 140px;
    height: 127px;
    justify-content: center;
    align-items: center;
    margin-right: 60px;
