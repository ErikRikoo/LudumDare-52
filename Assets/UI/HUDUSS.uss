.hud-tooltips {
    position: absolute;
    bottom: 150px;
    left: 840px;
    flex-direction: row;
    justify-content: space-between;
}

.hud-tooltip {
    width: 184px;
    height: 135px;
    background-image: url('project://database/Assets/UI/Textures/Tooltip-Background.png?fileID=2800000&guid=785c916670a0921429ca429f5ebfaeaa&type=3#Tooltip-Background');
    opacity: 0;
    transition-property: all, opacity;
    transition-duration: 0s, 300ms;
    transition-timing-function: ease, ease-out-circ;
    transition-delay: 0s, 0s;
    margin-right: -24px;
}

.hud-tooltip-label {
    width: 100%;
    height: 100%;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-top: 0;
    padding-bottom: 20px;
    font-size: 25px;
    color: rgb(66, 59, 44);
    -unity-text-outline-width: 0.5px;
    -unity-text-outline-color: rgba(0, 0, 0, 0.35);
    -unity-text-align: middle-center;
    padding-right: 15px;
    padding-left: 15px;
    white-space: normal;
    transition-property: all, opacity;
    transition-duration: 0s, 300ms;
    transition-timing-function: ease, ease-out-circ;
    transition-delay: 0s, 0s;
}

