    width: 100%;
    height: 100%;
    position: absolute;
}

.hud-weapon-slot-icon {
    position: absolute;
    width: 110%;
    height: 110%;
    -unity-background-scale-mode: scale-and-crop;
    bottom: 18px;
    left: 20px;
}

.hud-weapon-slot-label {
    position: absolute;
    font-size: 28px;
    color: rgb(255, 255, 255);
    bottom: 20px;
    -unity-text-outline-width: 1px;
    -unity-text-outline-color: rgba(0, 0, 0, 0.35);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

